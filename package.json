{
  "name": "oxo-monorepo",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "test": "pnpm -r test",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,js,json,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^2.8.8",
    "typescript": "^5.1.0"
  }
}
